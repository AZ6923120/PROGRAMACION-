#include <iostream>
using namespace std;

int main() {
    int n;

    cout << "Cuantos valores tendra el vector? ";
    cin >> n;

    int v[n];       
    int r[n];      

    cout << "Ingrese los " << n << " valores:\n";
    for (int i = 0; i < n; i++) {
        cin >> v[i];
    }

    for (int i = 0; i < n; i++) {
        r[i] = v[n - 1 - i];
    }

    cout << "Vector invertido:\n";
    for (int i = 0; i < n; i++) {
        cout << r[i] << " ";
    }

    return 0;
}
