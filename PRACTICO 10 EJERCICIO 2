#include <iostream>
using namespace std;

int main() {
    int n;

    cout << "Cuantos valores tendra el vector? ";
    cin >> n;

    int v[n];

    cout << "Ingrese los " << n << " valores:\n";
    for (int i = 0; i < n; i++) {
        cin >> v[i];
    }

    int minimo = v[0];

    for (int i = 1; i < n; i++) {
        if (v[i] < minimo) {
            minimo = v[i];
        }
    }

    cout << "El valor minimo es: " << minimo << endl;

    return 0;
}
