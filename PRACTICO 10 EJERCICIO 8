#include <iostream>
#include <vector>
#include <cmath> // para abs

using namespace std;

int main() {
    vector<int> A = {3, 8, 2, 10, 5};
    int maxDiff = 0;

    for(int i = 0; i < A.size() - 1; i++) {
        int diff = abs(A[i+1] - A[i]);
        if(diff > maxDiff) {
            maxDiff = diff;
        }
    }

    cout << "La diferencia mÃ¡s grande entre elementos consecutivos es: " << maxDiff << endl;
    return 0;
}
