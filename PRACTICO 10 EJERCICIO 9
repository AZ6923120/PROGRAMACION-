#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> A = {1,2,3};
    vector<int> B = {4,5,6};
    vector<int> C;

    for(int i : A) C.push_back(i);
    for(int i : B) C.push_back(i);

    cout << "Vector concatenado: ";
    for(int i : C) cout << i << " ";
    cout << endl;
    return 0;
}
