#include <iostream>
using namespace std;

int main() {
    int n;

    cout << "Cuantos valores tendra el vector? ";
    cin >> n;

    int v[n];

    cout << "Ingrese los " << n << " valores:\n";
    for (int i = 0; i < n; i++) {
        cin >> v[i];
    }

    int maximo = v[0];

    for (int i = 1; i < n; i++) {
        if (v[i] > maximo) {
            maximo = v[i];
        }
    }

    cout << "El valor maximo es: " << maximo << endl;

    return 0;
}
