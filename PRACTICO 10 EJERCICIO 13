#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> A = {1,2,3,4,5};

    for(int i = 0; i < A.size()/2; i++) {
        int temp = A[i];
        A[i] = A[A.size()-1-i];
        A[A.size()-1-i] = temp;
    }

    cout << "Vector invertido: ";
    for(int x : A) cout << x << " ";
    cout << endl;
}
