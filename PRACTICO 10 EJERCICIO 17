#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cout << "Cuantos numeros Fibonacci? "; cin >> n;
    vector<int> fib(n);
    fib[0] = 0;
    if(n>1) fib[1] = 1;
    for(int i=2;i<n;i++)
        fib[i] = fib[i-1] + fib[i-2];

    cout << "Fibonacci: ";
    for(int x : fib) cout << x << " ";
    cout << endl;
}
